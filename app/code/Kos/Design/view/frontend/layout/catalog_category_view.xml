<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.bottom.container">
            <block class="Magento\Cms\Block\Block" name="card-seo-banner" after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">card-seo-banner-catalog-category-view</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceContainer name="main.content">
            <block class="Magento\Framework\View\Element\Template" name="hs.global.js" template="Kos_Design::widget/global.phtml" after="-" />
        </referenceContainer>
    </body>
</page>